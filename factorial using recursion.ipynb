   "source": [
    "def factorial(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    return n*factorial(n-1)    \n",
    "num=int(input(\"enter the value:\"))\n",
    "value=factorial(num)\n",
    "print(value)"
   ]
