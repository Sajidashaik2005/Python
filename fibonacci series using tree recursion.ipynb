   "source": [
    "#fibonacci on tree recursions 0 1 1 2 3 5 8\n",
    "def fib(n):\n",
    "    if n<=1:\n",
    "        return n\n",
    "    return fib(n-1)+fib(n-2)\n",
    "terms=int(input(\"Enter number of terms to print:\"))\n",
    "print((\"fibonacci series...\"))\n",
    "for i in range(terms):\n",
    "    print(fib(i),end=' ')\n",
    "                "
   ]
